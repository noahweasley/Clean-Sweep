{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"camelcase": "error",
		"no-var": "error",
		"prefer-const": "warn",
		"prefer-template": "error",
		"no-console": "error",
		"no-unused-vars": "warn",
		"linebreak-style": [
			"error",
			"windows"
		],
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"eqeqeq": [
			"error",
			"always"
		],
		"max-len": [
			"warn",
			{
				"code": 125
			}
		]
	}
}
